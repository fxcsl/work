<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#bb111111" />
            <corners android:bottomLeftRadius="7.0dip" />
            <stroke android:width="0.5dip" android:color="#22ffffff" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#bb222222" />
            <corners android:bottomLeftRadius="7.0dip" />
            <stroke android:width="0.5dip" android:color="#44ffffff" />
        </shape>
    </item>
</selector>