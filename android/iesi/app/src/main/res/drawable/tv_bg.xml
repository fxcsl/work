<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff2A56C6" />
            <corners android:topLeftRadius="6.0dip" android:topRightRadius="6.0dip" android:bottomLeftRadius="6.0dip" android:bottomRightRadius="6.0dip" />
            <stroke android:width="0.5dip" android:color="#99ffffff" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="#662A56C6" />
            <corners android:topLeftRadius="6.0dip" android:topRightRadius="6.0dip" android:bottomLeftRadius="6.0dip" android:bottomRightRadius="6.0dip" />
            <stroke android:width="0.5dip" android:color="#99ffffff" />
        </shape>
    </item>
</selector>